{
  "name": "illionitgroup/laravel-szamlazzhu",
  "license": "MIT",
  "description": "Szamlazz.hu integration for Laravel",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "szamlazz.hu",
    "szamlazzhu",
    "currency"
  ],
  "require": {
    "php": "^7.1.3|^8.0 |^8.1",
    "laravel/framework": " 6.* | 7.* | 8.* | 9.* | 10.*"
  },
  "require-dev": {
  },
  "autoload": {
    "files": [
    ],
    "psr-4": {
      "IllionItGroup\\SzamlazzHu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IllionItGroup\\SzamlazzHu\\Tests\\": "test/",
    }
  },
  "suggest": {
    "illuminate/validation": "5.7.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "aliases": {
      },
      "providers": [
        "IllionItGroup\\SzamlazzHu\\Providers\\SzamlazzHuServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
